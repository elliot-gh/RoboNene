{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": false,
        "strict": false,
        "sourceMap": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "lib": ["es6"],
        "esModuleInterop": true,
        "resolveJsonModule": true,
    },
    "include": [
        "./",
        "./scripts"
    ]
}